    "react": "^15.0.0",
    "react-dom": "^15.0.0"
  },
  "devDependencies": {
    "bower": "^1.8.4",
    "pulp": "^12.4.2",
    "purescript": "^0.12.5",
    "purescript-psa": "^0.7.2",
    "rimraf": "^2.6.2"
  }
