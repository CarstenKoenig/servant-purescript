{
  "name": "pux-starter-app",
  "version": "6.0.0",
  "description": "Starter Pux application using webpack with hot-reloading.",
  "main": "support/index.js",
  "keywords": [
    "pux",
    "purescript-pux",
    "boilerplate",
    "starter-app"
  ],
  "scripts": {
